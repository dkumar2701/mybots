<robot name = "robot">
    <link name="Torso">
        <inertial>
            <origin xyz="0 0 1.2" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 1.2" rpy="0 0 0"/>
            <geometry>
                <box size="1 2 1" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 1.2" rpy="0 0 0"/>
            <geometry>
                <box size="1 2 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_RightBack" type="revolute">
       <parent link="Torso"/>
       <child  link="RightBack"/>
       <origin rpy="0 0 0" xyz="-0.25 -0.5 0.7" />
     <axis xyz="0 1 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="RightBack">
        <inertial>
            <origin xyz="0 0 -0.25" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 -0.25" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.5" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.25" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.5" />
            </geometry>
        </collision>
    </link>
</robot>